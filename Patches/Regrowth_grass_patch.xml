<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth 2</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="RG_GrassBase"]/plant</xpath>
					<value>
						<harvestYield>2</harvestYield>
						<harvestedThingDef>Hay</harvestedThingDef>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="RG_TallGrassBase"]/plant</xpath>
					<value>
						<harvestYield>2</harvestYield>
						<harvestedThingDef>Hay</harvestedThingDef>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>